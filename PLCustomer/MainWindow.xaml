<Window x:Class="PLCustomer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PLCustomer"
        mc:Ignorable="d"
        Title="Minip_Project" Height="640" Width="800"
        Icon="/img/droneIcon.ico"
        Background="Cyan"
        ResizeMode="CanMinimize">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0*"/>
            <RowDefinition Height="7*"/>
            <RowDefinition Height="149*"/>
        </Grid.RowDefinitions>
        <Frame x:Name="MainPage" NavigationUIVisibility="Hidden" Grid.RowSpan="3" Navigated="MainPage_Navigated" />
    </Grid>
</Window>
